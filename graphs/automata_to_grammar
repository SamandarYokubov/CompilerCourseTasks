digraph {
	S
	S -> S [label=0]
	B
	S -> B [label=1]
	A
	S -> A [label=2]
	f0 [label="" color=blue shape=circle]
	B -> f0 [label=1]
	0 [label="" shape=none]
	f0 -> 0 [color=blue]
	1 [label="" shape=none]
	A [color=blue]
	A -> 1 [color=blue]
	A -> B [label=0]
	2 [label="" shape=none]
	B [color=blue]
	B -> 2 [color=blue]
}
