digraph {
	S
	A
	B
	S -> A [label=0]
	S -> B [label=1]
	S [color=blue]
	2 [label="" shape=none]
	S -> 2 [color=blue]
	f3 [label="" color=blue shape=circle]
	A -> f3 [label="1,0"]
	3 [label="" shape=none]
	f3 -> 3 [color=blue]
	f4 [label="" color=blue shape=circle]
	B -> f4 [label="1,0"]
	4 [label="" shape=none]
	f4 -> 4 [color=blue]
}
